<!DOCTYPE html>
<html>
    <head>
        <title>FSD 2</title>
    </head>
    <body>
        <%- include('navbar'); %>
        <h2>Climate Survey Table</h2>
        <table>
            <tr>
              <th>Sl No</th>
              <th>Age</th>
              <th>Education</th>
              <th>Country</th>
              <th>Issue</th>
              <th>Imp</th>
              <th>Cause</th>
              <th>Impact</th>
              <th>Tackle</th>
              <th>Action</th>
            </tr>
            <% for(const book of model) { %>
            <tr>
              <td><%=book.pid %></td>
              <td><%=book.age %></td>
              <td><%=book.education %></td>
              <td><%=book.country %></td>
              <td><%=book.issue %></td>
              <td><%=book.imp %></td>
              <td><%=book.cause %></td>
              <td><%=book.impact %></td>
              <td><%=book.tackle %></td>
              <td><%=book.action %></td>
            </tr>
          <% } %>
        </table>
    </body>
</html>